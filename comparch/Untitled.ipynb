    "def shift_l(value,shift):\n",
    "    if value >= 0:\n",
    "        return value>>shift\n",
    "    else: \n",
    "        return (value+0x100000000)>>shift\n",
    "        \n",
    "print(shift_l(-1000, 3))\n",
    "print(shift_l(1000, 3))\n",
    "\n",
